    "nunjucks": "^3.1.3",
    "postcss-loader": "^2.0.6",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-hot-loader": "^4.3.3",
    "style-loader": "^0.18.2",
    "uglifyjs-webpack-plugin": "^1.2.7",
    "vue-loader": "^13.0.1",
