{
  "name": "market",
  "version": "1.0.0",
  "description": "",
  "main": "server.js",
  "scripts": {
    "start": "npm run assets & cross-env NODE_ENV=production NODE_PATH=. node ./bin/www.js",
    "start:dev": "npm run assets & cross-env NODE_PATH=. nodemon ./bin/www.js",
    "webpack": "rm -rf ./dist/js && npm run webpack:development",
    "webpack:development": "webpack --watch --progress --colors --env.development",
    "webpack:watch": "npm run webpack --progresss --colors --watch --env.development",
    "webpack:production": "npm run assets:production && cross-env NODE_ENV=production webpack -p --colors --progress --env.production",
    "database:init": "knex migrate:make setup",
    "database:apply": "knex migrate:latest",
    "assets:production": "rimraf dist && mkdir dist",
    "assets": "cp -r static/css static/images static/fonts dist/",
    "lint:js": "eslint --color --ext .js,.jsx ./static/js"
  },
  "repository": {
    "type": "git",
    "url": ""
  },
