    "babel-preset-es2016": "^6.24.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "body-parser": "^1.18.2",
    "classnames": "^2.2.5",
    "cross-env": "^5.1.4",
    "css-loader": "^0.28.4",
    "express": "^4.16.2",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^0.11.2",
